{
    "name": "mlk9/setting-laravel",
    "description": "Setting (encryption) for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Maleki",
            "email": "malekii24@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mlk9\\Setting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mlk9\\SettingPackage\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.2",
        "illuminate/support": "~6|~7|~8|~9|~10"
    },
    "require-dev" : {
        "laravel/pint": "^1.13.6",
        "phpstan/phpstan": "^1.10.47",
        "symfony/var-dumper": "^6.4.0|^7.0.1",
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.18"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mlk9\\Setting\\SettingServiceProvider"
            ],
            "aliases": {
                "Setting": "Mlk9\\Setting\\Facades\\Setting"
            }
        }
    }
}
